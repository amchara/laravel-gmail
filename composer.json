{
  "name": "amchara/laravel-gmail",
  "description": "Gmail API package for Laravel based on dacastro4/laravel-gmail",
  "keywords": [
    "api",
    "laravel",
    "Gmail"
  ],
  "license": "MIT",
  "authors": [{
    "name": "Alex & Georgi",
    "email": "tech@amchara.com",
    "homepage": "https://amchara.com"
  }],
  "require": {
    "php": "^7.2",
    "google/apiclient": "^2.4",
    "illuminate/auth": "^8.2",
    "illuminate/config": "^8.2",
    "illuminate/database": "^8.2",
    "illuminate/routing": "^8.2",
    "illuminate/session": "^8.2",
    "illuminate/support": "^8.2",
    "swiftmailer/swiftmailer": "~5.8|^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "mockery/mockery": "^1.0",
    "squizlabs/php_codesniffer": "~3.4",
    "orchestra/testbench": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "Amchara\\LaravelGmail\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Amchara\\LaravelGmail\\LaravelGmailServiceProvider"
      ],
      "aliases": {
        "LaravelGmail": "Amchara\\LaravelGmail\\Facade\\LaravelGmail"
      }
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
