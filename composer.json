{
  "name": "amchara/laravel-gmail",
  "description": "Gmail API package for Laravel based on dacastro4/laravel-gmail",
  "keywords": [
    "api",
    "laravel",
    "Gmail"
  ],
  "license": "MIT",
  "authors": [{
    "name": "Alex & Georgi",
    "email": "tech@amchara.com",
    "homepage": "https://amchara.com"
  }],
  "require": {
    "php": ">=5.6",
    "google/apiclient": "^2.0",
    "illuminate/auth": "~5.0|~5.1|~5.2|~5.3",
    "illuminate/config": "~5.0|~5.1|~5.2|~5.3",
    "illuminate/database": "~5.0|~5.1|~5.2|~5.3",
    "illuminate/routing": "~5.0|~5.1|~5.2|~5.3",
    "illuminate/session": "~5.0|~5.1|~5.2|~5.3",
    "illuminate/support": "~5.0|~5.1|~5.2|~5.3",
    "swiftmailer/swiftmailer": "~5.1|~5.4|~6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.5",
    "mockery/mockery": "~0.9",
    "squizlabs/php_codesniffer": "~2.0",
    "orchestra/testbench": "~3.5"
  },
  "autoload": {
    "psr-4": {
      "Amchara\\LaravelGmail\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Amchara\\LaravelGmail\\LaravelGmailServiceProvider"
      ],
      "aliases": {
        "LaravelGmail": "Amchara\\LaravelGmail\\Facade\\LaravelGmail"
      }
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}